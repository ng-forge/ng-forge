{
  "name": "@ng-forge/source",
  "type": "module",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "bash scripts/postinstall.sh",
    "build:libs": "nx run-many -t build -p dynamic-forms,dynamic-forms-material,dynamic-forms-bootstrap,dynamic-forms-primeng,dynamic-forms-ionic",
    "build:docs": "nx build docs",
    "build:examples": "nx run-many -t build -p material-examples,bootstrap-examples,primeng-examples,ionic-examples --configuration=production",
    "build:all:prod": "pnpm run build:libs && pnpm run build:docs && pnpm run build:examples",
    "prepare:deploy": "node scripts/prepare-deploy.js",
    "deploy:prep": "pnpm run build:all:prod && pnpm run prepare:deploy",
    "serve:all": "bash scripts/serve-with-examples.sh",
    "serve:ssr:docs": "node dist/apps/docs/server/server.mjs",
    "test": "nx run-many -t test",
    "test:ci": "nx run-many -t test --configuration=ci",
    "test:watch": "nx run-many -t test --watch",
    "lint": "nx run-many -t lint",
    "lint:fix": "nx run-many -t lint --fix",
    "e2e": "nx run-many -t e2e",
    "e2e:bootstrap": "nx run bootstrap-examples:e2e",
    "e2e:material": "nx run material-examples:e2e",
    "e2e:primeng": "nx run primeng-examples:e2e",
    "e2e:ionic": "nx run ionic-examples:e2e",
    "clean": "nx reset && rm -rf dist node_modules/.cache",
    "release:branch:patch": "bash scripts/create-release-branch.sh patch",
    "release:branch:minor": "bash scripts/create-release-branch.sh minor",
    "release:branch:major": "bash scripts/create-release-branch.sh major"
  },
  "private": true,
  "devDependencies": {
    "@analogjs/platform": "2.1.3",
    "@analogjs/vite-plugin-angular": "2.1.3",
    "@analogjs/vitest-angular": "2.1.3",
    "@angular-devkit/build-angular": "^21.0.4",
    "@angular-devkit/core": "^21.0.4",
    "@angular-devkit/schematics": "^21.0.4",
    "@angular/build": "^21.0.4",
    "@angular/cli": "^21.0.4",
    "@angular/compiler-cli": "^21.0.6",
    "@angular/language-service": "^21.0.6",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-angular": "^20.2.0",
    "@eslint/js": "^9.39.2",
    "@nx/angular": "22.3.3",
    "@nx/devkit": "22.3.3",
    "@nx/eslint": "22.3.3",
    "@nx/eslint-plugin": "22.3.3",
    "@nx/js": "22.3.3",
    "@nx/playwright": "22.3.3",
    "@nx/vite": "22.3.3",
    "@nx/vitest": "22.3.3",
    "@nx/web": "22.3.3",
    "@nx/workspace": "22.3.3",
    "@playwright/test": "1.57.0",
    "@schematics/angular": "~21.0.4",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.15.7",
    "@swc/helpers": "~0.5.11",
    "@types/express": "^5.0.6",
