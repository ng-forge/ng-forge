    "jsonc-eslint-parser": "^2.1.0",
    "lefthook": "^2.0.0",
    "ng-packagr": "^21.0.0",
    "nx": "22.3.3",
    "patch-package": "^8.0.1",
    "playwright": "1.57.0",
    "postcss": "^8.4.5",
    "postcss-url": "~10.1.3",
    "prettier": "^3.7.4",
