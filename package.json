{
  "name": "@ng-forge/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "nx run-many -t build",
    "build:libs": "nx run-many -t build -p dynamic-form,dynamic-form-material",
    "build:docs": "ng-doc api && nx build docs",
    "postinstall": "bash scripts/postinstall.sh",
    "patch:angular": "bash scripts/patch-angular-build.sh",
    "build:docs:prod": "ng-doc api && nx build docs --configuration=production",
    "test": "nx run-many -t test",
    "test:ci": "nx run-many -t test --configuration=ci --coverage",
    "test:watch": "nx run-many -t test --watch",
    "lint": "nx run-many -t lint",
    "lint:fix": "nx run-many -t lint --fix",
    "serve:docs": "nx serve docs",
    "serve:docs:prod": "nx serve docs --configuration=production",
    "clean": "nx reset && rm -rf dist node_modules/.cache",
    "affected:build": "nx affected -t build",
    "affected:test": "nx affected -t test",
    "affected:lint": "nx affected -t lint",
    "ci": "pnpm run build:libs && pnpm run test:ci",
    "docs:rebuild": "rm -rf dist/ng-doc && ng-doc api && ng build --project=docs --configuration=production || ng build apps/docs --configuration=production",
    "release:version": "nx release version",
    "release:version:patch": "nx release version patch",
    "release:version:minor": "nx release version minor",
    "release:version:major": "nx release version major",
    "release:publish": "nx release publish",
    "release:publish:dry-run": "nx release publish --dry-run",
    "release": "pnpm run build:libs && pnpm run test:ci && nx release version && nx release publish"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "~21.0.0-next.8",
    "@angular-devkit/core": "~21.0.0-next.8",
    "@angular-devkit/schematics": "~21.0.0-next.8",
    "@angular/build": "~21.0.0-next.8",
    "@angular/cli": "~21.0.0-next.8",
    "@angular/compiler-cli": "~21.0.0-next.8",
    "@angular/language-service": "~21.0.0-next.8",
    "@angular/material": "^21.0.0-next.9",
    "@eslint/js": "^9.8.0",
    "@nx/angular": "21.6.5",
    "@nx/devkit": "21.6.5",
    "@nx/eslint": "21.6.5",
    "@nx/eslint-plugin": "21.6.5",
    "@nx/jest": "21.6.5",
    "@nx/js": "21.6.5",
    "@nx/playwright": "21.6.5",
    "@nx/web": "21.6.5",
    "@nx/workspace": "21.6.5",
    "@playwright/test": "^1.36.0",
    "@schematics/angular": "~20.3.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.13.3",
    "@swc/helpers": "~0.5.11",
    "@types/jest": "^29.5.12",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "22.12.0",
    "@typescript-eslint/utils": "^8.40.0",
    "angular-eslint": "^20.3.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-playwright": "^1.6.2",
    "glob": "^11.0.3",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-preset-angular": "~15.0.3",
    "jest-util": "^29.7.0",
    "jsonc-eslint-parser": "^2.1.0",
    "lefthook": "^2.0.0",
    "ng-packagr": "21.0.0-next.4",
    "nx": "21.6.5",
    "patch-package": "^8.0.1",
    "postcss": "^8.4.5",
    "postcss-url": "~10.1.3",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.0.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.40.0",
    "verdaccio": "^6.0.5"
  },
  "dependencies": {
    "@angular/animations": "21.0.0-next.8",
    "@angular/cdk": "~21.0.0-next.9",
    "@angular/common": "~21.0.0-next.8",
    "@angular/compiler": "~21.0.0-next.8",
    "@angular/core": "~21.0.0-next.8",
    "@angular/forms": "~21.0.0-next.8",
    "@angular/platform-browser": "~21.0.0-next.8",
    "@angular/platform-browser-dynamic": "~21.0.0-next.8",
    "@angular/router": "~21.0.0-next.8",
    "@ng-doc/app": "^20.1.0",
    "@ng-doc/builder": "^20.1.0",
    "@ng-doc/core": "^20.1.0",
    "@ng-doc/ui-kit": "^20.1.0",
    "date-fns": "^4.1.0",
    "lodash-es": "^4.17.21",
    "ngxtension": "^6.0.0",
    "rxjs": "~7.8.0",
    "shiki": "^3.13.0",
    "zone.js": "~0.15.0"
  },
  "packageManager": "pnpm@8.15.1",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "nx": {
    "includedScripts": []
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "nx"
    ],
    "ignoredBuiltDependencies": [
      "unrs-resolver"
    ],
    "peerDependencyRules": {
      "allowedVersions": {
        "@angular/core": "21",
        "@angular/common": "21",
        "@angular/compiler": "21",
        "@angular/platform-browser": "21",
        "@angular/platform-browser-dynamic": "21",
        "@angular/router": "21",
        "@angular/forms": "21"
      }
    }
  }
}
