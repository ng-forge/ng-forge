{
  "name": "@ng-forge/source",
  "type": "module",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "nx run-many -t build",
    "build:libs": "nx run-many -t build -p dynamic-form,dynamic-form-material,dynamic-form-primeng,dynamic-form-ionic",
    "build:docs": "nx build docs",
    "postinstall": "bash scripts/postinstall.sh",
    "patch:angular": "bash scripts/patch-angular-build.sh",
    "build:docs:prod": "nx build docs --configuration=production",
    "build:examples": "nx run-many -t build -p material-examples,bootstrap-examples,primeng-examples,ionic-examples --configuration=production",
    "build:all:prod": "pnpm run build:libs && pnpm run build:docs:prod && pnpm run build:examples",
    "prepare:deploy": "node scripts/prepare-deploy.js",
    "deploy:prep": "pnpm run build:all:prod && pnpm run prepare:deploy",
    "serve:all": "bash scripts/serve-with-examples.sh",
    "test": "nx run-many -t test",
    "test:ci": "nx run-many -t test --configuration=ci --coverage",
    "test:watch": "nx run-many -t test --watch",
    "lint": "nx run-many -t lint",
    "lint:fix": "nx run-many -t lint --fix",
    "clean": "nx reset && rm -rf dist node_modules/.cache",
    "affected:build": "nx affected -t build",
    "affected:test": "nx affected -t test",
    "affected:lint": "nx affected -t lint",
    "ci": "pnpm run build:libs && pnpm run test:ci",
    "docs:rebuild": "rm -rf dist/ng-doc && ng-doc api && ng build --project=docs --configuration=production || ng build apps/docs --configuration=production",
    "release:version": "nx release version",
    "release:version:patch": "nx release version patch",
    "release:version:minor": "nx release version minor",
    "release:version:major": "nx release version major",
    "release:publish": "nx release publish",
    "release:publish:dry-run": "nx release publish --dry-run",
    "release": "pnpm run build:libs && pnpm run test:ci && nx release version && nx release publish"
  },
  "private": true,
  "devDependencies": {
    "@analogjs/platform": "^1.21.3",
    "@analogjs/vite-plugin-angular": "^1.21.3",
    "@analogjs/vitest-angular": "^1.21.3",
    "@angular-devkit/build-angular": "21.0.0",
    "@angular-devkit/core": "21.0.0",
    "@angular-devkit/schematics": "21.0.0",
    "@angular/build": "21.0.0",
    "@angular/cli": "21.0.0",
    "@angular/compiler-cli": "21.0.0",
    "@angular/language-service": "21.0.0",
    "@angular/material": "^21.0.0",
    "@eslint/js": "^9.8.0",
    "@nx/angular": "22.1.0",
    "@nx/devkit": "22.1.0",
    "@nx/eslint": "22.1.0",
    "@nx/eslint-plugin": "22.1.0",
    "@nx/js": "22.1.0",
    "@nx/playwright": "22.1.0",
    "@nx/vite": "22.1.0",
    "@nx/web": "22.1.0",
    "@nx/workspace": "22.1.0",
    "@playwright/test": "^1.56.1",
    "@schematics/angular": "~20.3.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.13.3",
    "@swc/helpers": "~0.5.11",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "22.12.0",
    "@typescript-eslint/utils": "^8.40.0",
    "@vitest/browser": "4.0.10",
    "@vitest/browser-playwright": "4.0.10",
    "@vitest/coverage-istanbul": "4.0.10",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.0",
    "angular-eslint": "^20.3.0",
    "autoprefixer": "^10.4.0",
    "esbuild": "^0.25.12",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-playwright": "^1.6.2",
    "glob": "^11.0.3",
    "jiti": "2.4.2",
    "jsdom": "^27.0.1",
    "jsonc-eslint-parser": "^2.1.0",
    "lefthook": "^2.0.0",
    "ng-packagr": "^20.3.0",
    "nx": "22.1.0",
    "patch-package": "^8.0.1",
    "playwright": "^1.56.1",
    "postcss": "^8.4.5",
    "postcss-url": "~10.1.3",
    "prettier": "^3.6.2",
    "swc-loader": "0.1.15",
    "tailwindcss": "^3.0.2",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.40.0",
    "vite": "^7.1.11",
