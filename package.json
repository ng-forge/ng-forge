    "@ng-doc/ui-kit": "^20.1.0",
    "@primeuix/themes": "^2.0.2",
    "@vercel/analytics": "^1.6.1",
    "bootstrap": "^5.3.8",
    "date-fns": "^4.1.0",
    "express": "^5.2.1",
    "lodash-es": "^4.17.21",
    "ngxtension": "^7.0.2",
    "primeicons": "^7.0.0",
    "primeng": "^21.0.1",
    "rxjs": "~7.8.0",
    "shiki": "^1.10.3"
  },
  "packageManager": "pnpm@8.15.1",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "nx": {
    "includedScripts": []
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "nx"
    ],
    "ignoredBuiltDependencies": [
      "unrs-resolver"
    ],
    "peerDependencyRules": {
      "allowedVersions": {
        "@angular/core": "21",
        "@angular/common": "21",
        "@angular/compiler": "21",
        "@angular/platform-browser": "21",
        "@angular/platform-browser-dynamic": "21",
        "@angular/router": "21",
        "@angular/forms": "21"
      }
    }
  }
