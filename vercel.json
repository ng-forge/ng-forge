{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm run deploy:prep",
  "outputDirectory": "dist/deploy",
  "git": {
    "deploymentEnabled": true
  },
  "ignoreCommand": "[ \"$VERCEL_GIT_COMMIT_REF\" = \"main\" ]",
  "redirects": [
    {
      "source": "/",
      "destination": "/dynamic-forms/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/dynamic-forms/examples/material/:path((?!.*\\.).+)",
      "destination": "/dynamic-forms/examples/material/index.html"
    },
    {
      "source": "/dynamic-forms/examples/bootstrap/:path((?!.*\\.).+)",
      "destination": "/dynamic-forms/examples/bootstrap/index.html"
    },
    {
      "source": "/dynamic-forms/examples/primeng/:path((?!.*\\.).+)",
      "destination": "/dynamic-forms/examples/primeng/index.html"
    },
    {
      "source": "/dynamic-forms/examples/ionic/:path((?!.*\\.).+)",
      "destination": "/dynamic-forms/examples/ionic/index.html"
    },
    {
      "source": "/dynamic-forms/:path((?!.*\\.).+)",
      "destination": "/dynamic-forms/index.html"
    },
    {
      "source": "/assets/:path*",
      "destination": "/dynamic-forms/assets/:path*"
    }
  ],
  "headers": [
    {
      "source": "/dynamic-forms/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/dynamic-forms/:path*.(js|css|woff2|png|jpg|svg|ico)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
