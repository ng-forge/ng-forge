{
  "name": "@ng-forge/dynamic-forms-zod",
  "version": "0.4.0",
  "description": "Zod schemas for validating @ng-forge/dynamic-forms configurations.",
  "type": "module",
  "license": "MIT",
  "author": "ng-forge",
  "homepage": "https://ng-forge.com/dynamic-forms/zod",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ng-forge/ng-forge.git",
    "directory": "packages/dynamic-forms-zod"
  },
  "bugs": {
    "url": "https://github.com/ng-forge/ng-forge/issues"
  },
  "keywords": [
    "zod",
    "schema",
    "validation",
    "dynamic-forms",
    "ng-forge",
    "angular",
    "json-schema",
    "mcp",
    "ai",
    "llm"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=20"
  },
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js",
      "default": "./src/index.js"
    },
    "./material": {
      "types": "./material/src/index.d.ts",
      "import": "./material/src/index.js",
      "default": "./material/src/index.js"
    },
    "./bootstrap": {
      "types": "./bootstrap/src/index.d.ts",
      "import": "./bootstrap/src/index.js",
      "default": "./bootstrap/src/index.js"
    },
    "./primeng": {
      "types": "./primeng/src/index.d.ts",
      "import": "./primeng/src/index.js",
      "default": "./primeng/src/index.js"
    },
    "./ionic": {
      "types": "./ionic/src/index.d.ts",
      "import": "./ionic/src/index.js",
      "default": "./ionic/src/index.js"
    },
    "./mcp": {
      "types": "./mcp/src/index.d.ts",
      "import": "./mcp/src/index.js",
      "default": "./mcp/src/index.js"
    }
  },
  "peerDependencies": {
    "zod": "^3.24.0"
  },
  "dependencies": {
    "tslib": "^2.3.0",
    "zod-to-json-schema": "^3.23.0"
  }
}
