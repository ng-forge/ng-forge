{
  "name": "dynamic-forms-bootstrap",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/dynamic-forms-bootstrap/src",
  "prefix": "df-bs",
  "projectType": "library",
  "implicitDependencies": ["dynamic-forms"],
  "release": {
    "version": {
      "manifestRootsToUpdate": ["{projectRoot}", "dist/{projectRoot}"],
      "currentVersionResolver": "git-tag",
      "fallbackCurrentVersionResolver": "disk"
    }
  },
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "packages/dynamic-forms-bootstrap/ng-package.json",
        "tsConfig": "packages/dynamic-forms-bootstrap/tsconfig.lib.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "packages/dynamic-forms-bootstrap/tsconfig.lib.prod.json"
        },
        "development": {}
      },
      "defaultConfiguration": "production"
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "type-test": {
      "executor": "@nx/vitest:test",
      "options": {
        "configFile": "packages/dynamic-forms-bootstrap/vite.config.ts"
      }
    }
  }
}
